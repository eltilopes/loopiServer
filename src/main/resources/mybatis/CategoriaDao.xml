<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="br.com.aio.model.repository.dao.CategoriaDao">
	<select id="getCatgorias"
			resultType="br.com.aio.model.entity.Categoria">
			
			SELECT 
				ci_categoria id, 
				ds_categoria descricao,
				ds_url_imagem urlImagem
  			FROM tb_categoria					
	</select>
</mapper>